{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prettify": "prettier --log-level log --ignore-path ./.prettierignore --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss}\"",
